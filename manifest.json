{
  "name": "Doctolib Chatbot",
  "version": "1.0",
  "description": "Chatbrerie",
  "manifest_version": 2,
  "content_scripts": [
   {
     "matches": ["https://www.doctolib.fr/*"],
     "css": ["src/assets/stylesheets/chatbot.css", "node_modules/bootstrap/dist/css/bootstrap.min.css"],
     "js": ["dist/bundle.js", "dist/524.bundle.js"]
   }
  ],
  "permissions": ["storage", "unlimitedStorage"],
  "web_accessible_resources": [
      "src/data/word-lists.txt",
      "src/data/output.json",
      "src/data/js-model/model.json",
      "src/data/js-model/group1-shard1of1.bin",
      "src/data/vaccination-quiz.json",

      "src/views/chatbot.html",
      "src/views/client-message.mustache.html",
      "src/views/bot-message.mustache.html",
      "src/views/quiz-question.mustache.html",
      "src/views/form-message.mustache.html",
      "src/views/image.mustache.html",
      "src/views/quiz-nav.html",
      "src/views/bot-base.mustache.html",
      "src/views/client-base.mustache.html",

      "src/assets/images/bye.gif",
      "src/assets/images/dontknow.gif",
      "src/assets/images/client-avatar.png",
      "src/assets/images/bot-avatar.png"
  ]
}
